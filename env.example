# Okta Configuration
OKTA_ORG_URL=https://your-org.okta.com
OKTA_API_TOKEN=your_api_token_here

# Alternative OAuth 2.0 Configuration (recommended)
# OKTA_CLIENT_ID=your_client_id
# OKTA_CLIENT_SECRET=your_client_secret
# OKTA_SCOPES=okta.users.manage,okta.apps.manage,okta.logs.read

# MCP Server Configuration
MCP_SERVER_NAME=okta-support
MCP_SERVER_VERSION=0.1.0
MCP_LOG_LEVEL=INFO

# Optional: Rate limiting and timeouts
OKTA_RATE_LIMIT=100
OKTA_TIMEOUT_SECONDS=30

# Development settings
DEVELOPMENT_MODE=false
LOG_STRUCTURED=true